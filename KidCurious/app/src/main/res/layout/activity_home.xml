<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/xanhhome"
    tools:context=".Home">

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/h1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/view6"
        android:layout_width="128sp"
        android:layout_height="133sp"
        android:background="@drawable/h6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.871" />

    <View
        android:id="@+id/view7"
        android:layout_width="280sp"
        android:layout_height="186sp"
        android:background="@drawable/h10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view8"
        android:layout_width="264sp"
        android:layout_height="444sp"
        android:layout_marginStart="224dp"
        android:background="@drawable/h10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view9"
        android:layout_width="239sp"
        android:layout_height="315sp"
        android:layout_marginStart="188dp"
        android:background="@drawable/h10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view4"
        android:layout_width="267sp"
        android:layout_height="match_parent"
        android:background="@drawable/h2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <FrameLayout
        android:layout_width="118dp"
        android:layout_height="82dp"
        android:layout_gravity="center"
        android:rotation="20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.645"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07">

        <!-- Ảnh vector khung hình con vật -->
        <Button
            android:id="@+id/btnA"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:background="@drawable/h8"
            android:scaleType="fitXY"
            app:backgroundTint="@null" />

        <!-- Chữ nằm giữa -->
        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:fontFamily="@font/summarynotes"
            android:gravity="center"
            android:text="Animal"
            android:textColor="@color/hongbtnhome"
            android:textSize="25dp"
            android:textStyle="bold" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="118dp"
        android:layout_height="82dp"
        android:layout_gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.645"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.724">

        <!-- Ảnh vector khung hình con vật -->
        <Button
            android:id="@+id/btnN"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:background="@drawable/h8"
            android:scaleType="fitXY"
            app:backgroundTint="@null" />

        <!-- Chữ nằm giữa -->
        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:fontFamily="@font/summarynotes"
            android:gravity="center"
            android:text="Number"
            android:textColor="@color/hongbtnhome"
            android:textSize="25dp"
            android:textStyle="bold" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="118dp"
        android:layout_height="82dp"
        android:layout_gravity="center"
        android:rotation="350"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.945"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.429">

        <!-- Ảnh vector khung hình con vật -->
        <Button
            android:id="@+id/btnF"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:background="@drawable/h8"
            android:scaleType="fitXY"
            app:backgroundTint="@null" />

        <!-- Chữ nằm giữa -->
        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:fontFamily="@font/summarynotes"
            android:gravity="center"
            android:text="Flag"
            android:textColor="@color/hongbtnhome"
            android:textSize="25dp"
            android:textStyle="bold" />
    </FrameLayout>

    <TextView
        android:id="@+id/txtHello"
        android:layout_width="121dp"
        android:layout_height="122dp"
        android:background="@drawable/h7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="41sp"
        android:layout_height="54sp"
        android:background="@drawable/h5"
        android:rotation="10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.867" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="41sp"
        android:layout_height="54sp"
        android:background="@drawable/h4"
        android:rotation="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.534" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/h3"
        android:rotation="295"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.305" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/h9"
        android:rotation="155"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.521" />


</androidx.constraintlayout.widget.ConstraintLayout>